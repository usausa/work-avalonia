<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sn="clr-namespace:Smart.Navigation;assembly=Smart.Navigation.Avalonia"
        xmlns:sr="clr-namespace:Smart.Avalonia.Resolver;assembly=Smart.Avalonia"
        xmlns:views="clr-namespace:AvaloniaApp.Views"
        mc:Ignorable="d"
        x:Class="AvaloniaApp.Views.MainWindow"
        x:DataType="{x:Type views:MainWindowViewModel}"
        sr:DataContextResolver.Type="views:MainWindowViewModel"
        Title="AvaloniaApp"
        Icon="/Assets/avalonia-logo.ico"
        Width="640" Height="480"
        d:DesignWidth="640" d:DesignHeight="480"
        CanResize="False">

    <Canvas sn:NavigationContainer.Navigator="{Binding Navigator}" />

</Window>
